<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="run.aiwan.dao.UserDao">

    <select id="queryUser" resultType="run.aiwan.entity.User">
        select uid,uname from todo_user where uname = #{uname}
    </select>

    <insert id="addUser">
        insert into todo_user(uname) values(#{uname})
    </insert>
</mapper>